{
  "name": "kujoman",
  "version": "0.1.0",
  "description": "kubernetes job manager",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "nodemon": "nodemon -V -e js,json,pug,vue index.js"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "node_modules/*"
    ],
    "ext": [
