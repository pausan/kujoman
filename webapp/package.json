  "author": "Pau Sanchez",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/datastore": "^1.4.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "pug": "^2.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "mocha-list-tests": "^1.0.1",
    "nodemon": "^1.17.2"
  }
}
